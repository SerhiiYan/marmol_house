{
  "version": 2,
  "name": "marmolhouse",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(?!api/.*|assets/.*|_next/.*|favicon.ico|sitemap.xml|robots.txt)(.*)",
      "dest": "/index.html",
      "headers": {
        "X-Prerender-Token": "$PRERENDER_TOKEN",
        "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    }
  ]
}