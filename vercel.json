{
  "version": 2,
  "name": "marmolhouse",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "X-Prerender-Token": "$PRERENDER_TOKEN",
        "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate"
      }
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://marmolhouse.vercel.app/$1",
      "permanent": true,
      "statusCode": 301
    }
  ]
}